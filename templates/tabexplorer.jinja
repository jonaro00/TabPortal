{% extends "tabportal.jinja" %}
{% block title %}Tabs in {{ url_for(endpoint) }} | {{ super() }}{% endblock %}
{% block head %}
  <link rel="stylesheet" href="{{ url_for('static', filename='tabExplorer.css') }}">
{% endblock %}
{% block content %}
  <div class="tp-explorer">
    <h2>Tabs</h2>
    <ul>
      {%- for ent in entries %}
        <li>
          <a href="{{ url_for(endpoint, path=ent.path) }}">{{ ent.name }}</a>
        </li>
      {% endfor -%}
    </ul>
  </div>
{% endblock %}
